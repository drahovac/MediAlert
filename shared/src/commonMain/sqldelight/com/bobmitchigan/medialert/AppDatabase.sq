CREATE TABLE Medicine (
    id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    name TEXT NOT NULL,
    blisterPacks TEXT NOT NULL
);

insertMedicine:
INSERT INTO Medicine(name, blisterPacks)
VALUES(?, ?);

removeAllMedicines:
DELETE FROM Medicine;

selectAllMedicines:
SELECT *
FROM Medicine;

selectMedicineById:
SELECT *
FROM Medicine
WHERE id = ?
LIMIT 1;